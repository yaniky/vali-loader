{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose" : true }],
      "@babel/plugin-transform-runtime",
      "syntax-dynamic-import",
    ]
  }